{
  "name": "vocable",
  "version": "1.0.0",
  "description": "Vocable exercise tool",
  "main": "index.js",
  "scripts": {
    "start": "run-p client-dev-server build-server watch-server",
    "build-client": "webpack --progress --colors --config webpack.config.babel.js",
    "client-dev-server": "webpack-dev-server --progress --colors",
    "build-server": "babel src/server --out-dir build/server --watch",
    "watch-server": "nodemon \"./build/server/server.js\" --watch \"./build/server\""
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nattelog/vocable.git"
  },
  "keywords": [],
  "author": "Natanael Log <natte.log@gmail.com> (www.natanael.se)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nattelog/vocable/issues"
  },
  "homepage": "https://github.com/nattelog/vocable#readme",
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-register": "^6.24.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "webpack": "2.2.1"
  }
}
